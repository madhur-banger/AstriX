{
    "Version": "2012-10-17",
    "Statement": [
      {
        "Sid": "AllowRootAccountRecovery",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::586794439017:root"
        },
        "Action": "kms:*",
        "Resource": "*"
      },
      {
        "Sid": "AllowAccountAdminKeyManagement",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::586794439017:role/aws-reserved/sso.amazonaws.com/AWSReservedSSO_AccountAdmin_4504fa524ccd4714"
        },
        "Action": [
          "kms:Create*",
          "kms:Describe*",
          "kms:Enable*",
          "kms:List*",
          "kms:PutKeyPolicy",
          "kms:Update*",
          "kms:Disable*",
          "kms:Get*",
          "kms:Delete*",
          "kms:ScheduleKeyDeletion",
          "kms:CancelKeyDeletion"
        ],
        "Resource": "*"
      },
      {
        "Sid": "AllowTerraformProvisionerUsage",
        "Effect": "Allow",
        "Principal": {
          "AWS": "arn:aws:iam::586794439017:role/aws-reserved/sso.amazonaws.com/AWSReservedSSO_TerraformProvisioner_340e75aafc4301ac"
        },
        "Action": [
          "kms:Encrypt",
          "kms:Decrypt",
          "kms:GenerateDataKey"
        ],
        "Resource": "*"
      }
    ]
  }
  